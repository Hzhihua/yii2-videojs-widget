{
    "name": "hzhihua/yii2-videojs-widget",
    "description": "yii2 videojs 小部件",
    "keywords": [
        "yii2-videojs-widget",
        "yii2-widget",
        "videojs"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/hzhihua/yii2-videojs-widget/issues",
        "wiki": "https://github.com/hzhihua/yii2-videojs-widget/wiki/",
        "source": "https://github.com/hzhihua/yii2-videojs-widget"
    },
    "authors": [
        {
            "name": "hzhihua",
            "email": "cnzhihua@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "npm-asset/video.js": "~v6.7.0"
    },
    "autoload": {
    	"psr-4": {
    		"hzhihua\\videojs\\": "src/"
    	}
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "extra": {
        "installer-types": ["bower-asset", "npm-asset"]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
