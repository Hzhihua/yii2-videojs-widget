{
    "name": "hzhihua/yii2-videojs-widget",
    "description": "yii2 videojs 小部件",
    "keywords": [
        "yii2-videojs-widget",
        "yii2-widget",
        "videojs"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/hzhihua/yii2-videojs-widget/issues",
        "wiki": "https://github.com/hzhihua/yii2-videojs-widget/wiki/",
        "source": "https://github.com/hzhihua/yii2-videojs-widget"
    },
    "authors": [
        {
            "name": "hzhihua",
            "email": "hzhihuaemail@163.com"
        }
    ],
    "require": {
    	"yiisoft/yii2": "~2.0"
    },
    "autoload": {
    	"psr-4": {
    		"hzhihua\\videojs\\": "src/"
    	}
    },
    "scripts": {
        "post-install-cmd": [
            "npm install --save video.js",
            "npm install --save videojs-flash"
        ],
        "post-update-cmd": [
            "npm install --save video.js",
            "npm install --save videojs-flash"
        ]
    }
}
