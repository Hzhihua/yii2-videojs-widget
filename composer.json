{
    "name": "hzhihua/yii2-videojs-widget",
    "description": "yii2 videojs 小部件",
    "keywords": [
        "yii2-videojs-widget",
        "yii2-widget",
        "videojs"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/hzhihua/yii2-videojs-widget/issues",
        "wiki": "https://github.com/hzhihua/yii2-videojs-widget/wiki/",
        "source": "https://github.com/hzhihua/yii2-videojs-widget"
    },
    "authors": [
        {
            "name": "hzhihua",
            "email": "hzhihuaemail@163.com"
        }
    ],
    "require": {
    	"yiisoft/yii2": "~2.0",
        "npm-asset/video.js": "~6.0",
        "npm-asset/videojs-flash": "~2.0"
    },
    "autoload": {
    	"psr-4": {
    		"hzhihua\\videojs\\": "src/"
    	}
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset":{
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    }
}
